<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="temp">

<select id="securitybyid" parameterType="String" resultType="kr.co.korea.beans.SecurityMemberBeanDetails">
select midx,mname,mamount,mid,mpw,mdrop,mlevel
from member
where mid=#{mid}
</select>

</mapper>